<a href="#" class="button-list" role="button">Transactions<br><br>
  <table class="table">
    <% @notes.each do |transaction| %>
        <tr>
          <% if transaction.transaction_date.present? %>
            <td><%= transaction.transaction_date.strftime("%m-%d-%Y") %></td>
          <% else %>
          <td><%= transaction.created_at.strftime("%m-%d-%Y") %></td>
          <% end %>
          <td><%= transaction.explanation %></td>
          <td><%= link_to "#{number_to_currency(transaction.amount)}", transaction %></td>
        </tr>
      
    <% end %>
   
  </table>
  
  </a>
  <%= will_paginate @notes %> <br>