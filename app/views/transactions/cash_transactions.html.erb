<div class="page-content-col">
  <div class="col-md-6 col-md-offset-3">
    <a href="#" class="button-list" role="button">Transactions<br><br>
      <table class="table">
        <% @transactions.each do |transaction| %>
            <tr>
              <td><%= transaction.created_at.strftime("%m-%d-%Y") %></td>
              <td><%= transaction.explanation %></td>
              <td><%= link_to "#{transaction.amount}", transaction %></td>
            </tr>
          </a>
        <% end %>
      </table>
    <%= link_to 'New Transaction', new_account_transaction_path(@account) %>
  </div>
</div>

